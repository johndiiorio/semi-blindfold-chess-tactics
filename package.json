{
  "name": "semi-blindfold-chess",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node src/server/server.js",
    "build": "run-s build:schema build:relay build:client",
    "dev:app": "run-p dev:client dev:server",
    "dev:relay": "run-s build:schema 'build:relay --watch'",
    "dev:client": "react-scripts start",
    "dev:server": "cross-env NODE_ENV=development node src/server/server.js",
    "build:client": "react-scripts build",
    "build:relay": "relay-compiler --src ./src/client/ --schema ./src/__generated__/schema.graphql --artifactDirectory ./src/__generated__/relay",
    "build:schema": "node ./src/server/graphql/buildSchema.js"
  },
  "dependencies": {
    "@material-ui/core": "~4.4.0",
    "@material-ui/icons": "~4.2.1",
    "@material-ui/styles": "~4.3.3",
    "body-parser": "~1.19.0",
    "classnames": "~2.2.6",
    "cross-env": "~5.2.1",
    "express": "~4.17.1",
    "express-graphql": "~0.9.0",
    "graphql": "~14.5.4",
    "graphql-relay": "~0.6.0",
    "helmet": "~3.20.1",
    "lodash": "~4.17.15",
    "prop-types": "~15.7.2",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-relay": "~5.0.0",
    "react-router-dom": "~5.0.1",
    "react-scripts": "~3.1.1",
    "sqlite3": "~4.1.0"
  },
  "devDependencies": {
    "babel-plugin-relay": "~5.0.0",
    "eslint": "~6.3.0",
    "eslint-config-prettier": "~6.1.0",
    "eslint-plugin-prettier": "~3.1.0",
    "eslint-plugin-react": "~7.14.3",
    "eslint-plugin-react-hooks": "~2.0.1",
    "eslint-plugin-relay": "~1.3.10",
    "npm-run-all": "~4.1.5",
    "prettier": "~1.18.2",
    "relay-compiler": "~5.0.0"
  },
  "proxy": "http://localhost:8080",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babelMacros": {
    "relay": {
      "artifactDirectory": "./src/__generated__/relay"
    }
  }
}
